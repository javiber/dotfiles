# two-finger right click
xinput set-prop 13 --type=int 296 0 1

# tap-click
xinput set-prop 13 280 1

# set keyboard layout
setxkbmap -layout us -variant altgr-intl -option nodeadkeys

# re-map tab
xmodmap ~/.Xmodmap

# fix dpi
#xrandr --dpi 200

# Change reslution
randr --output eDP-1 --mode 2048x1152
